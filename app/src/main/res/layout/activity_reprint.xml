<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >

<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Reprint">

    <LinearLayout
        android:id="@+id/linear_papa_reprint"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/formulario_reprint"
            android:layout_width="325dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center|top"
            android:orientation="vertical"
            android:padding="5dp"
            android:layout_marginTop="10dp"
            android:elevation="20dp"
            android:background="#fff">

            <View
                android:layout_width="fill_parent"
                android:layout_height="8dp"
                android:layout_marginTop="5dp"
                android:background="#CCDEC2"></View>

            <LinearLayout
                android:id="@+id/linear_reprint_sku_cod"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="5dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView_no_sku_cod_reprint"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/text_margin"
                    android:text="@string/reprint_sku_cod"
                    android:textSize="30sp"
                    app:fontFamily="@font/bebasneue_regular" />

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="0dp"
                    android:layout_height="45dp"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1"
                    android:contentDescription="@string/contentDescriptionImages"
                    app:srcCompat="@drawable/icon_edit" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/linear_editText_one_reprint"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <EditText
                    android:id="@+id/edittext_reprint_sku_cod"
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:hint="@string/hint_edit_text"
                    android:inputType="text"
                    app:fontFamily="@font/bebasneue_regular" />

            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="8dp"
                android:layout_marginTop="5dp"
                android:background="#CCDEC2"></View>

            <LinearLayout
                android:id="@+id/linear_descripcion_reprint"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="5dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView_descripcion_reprint"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/text_margin"
                    android:text="@string/reprint_descripcion"
                    android:textSize="30sp"
                    app:fontFamily="@font/bebasneue_regular" />

                <ImageView
                    android:id="@+id/imageView7"
                    android:layout_width="0dp"
                    android:layout_height="45dp"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1"
                    android:contentDescription="@string/contentDescriptionImages"
                    app:srcCompat="@drawable/icon_edit" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/linear_editText_three_reprint"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <EditText
                    android:id="@+id/edittext_descripcion_reprint"
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:hint="@string/hint_edit_text"
                    android:inputType="text"
                    app:fontFamily="@font/bebasneue_regular" />

            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="8dp"
                android:layout_marginTop="5dp"
                android:background="#CCDEC2"></View>

            <LinearLayout
                android:id="@+id/linear_frescura_reprint"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="5dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView_frescura_reprint"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/text_margin"
                    android:text="@string/reprint_frescura"
                    android:textSize="30sp"
                    app:fontFamily="@font/bebasneue_regular" />

                <ImageView
                    android:id="@+id/imageView8"
                    android:layout_width="0dp"
                    android:layout_height="45dp"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1"
                    android:contentDescription="@string/contentDescriptionImages"
                    app:srcCompat="@drawable/icon_edit" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/linear_editText_four_reprint"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <EditText
                    android:id="@+id/edittext_frescura_reprint"
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:hint="@string/hint_edit_text"
                    android:inputType="text"
                    app:fontFamily="@font/bebasneue_regular" />

            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="8dp"
                android:layout_marginTop="5dp"
                android:background="#CCDEC2"></View>

            <LinearLayout
                android:id="@+id/linear_consecutivo_i_reprint"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="5dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView_consecutivo_i_reprint"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/text_margin"
                    android:text="@string/reprint_consecutivo_i"
                    android:textSize="30sp"
                    app:fontFamily="@font/bebasneue_regular" />

                <ImageView
                    android:id="@+id/imageView9"
                    android:layout_width="0dp"
                    android:layout_height="45dp"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1"
                    android:contentDescription="@string/contentDescriptionImages"
                    app:srcCompat="@drawable/icon_edit" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/linear_editText_five_reprint"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <EditText
                    android:id="@+id/edittext_consecutivo_i_reprint"
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:hint="@string/hint_edit_text"
                    android:inputType="text"
                    app:fontFamily="@font/bebasneue_regular" />

            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="8dp"
                android:layout_marginTop="5dp"
                android:background="#CCDEC2"></View>

            <LinearLayout
                android:id="@+id/linear_consecutivo_f_reprint"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="5dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView_consecutivo_f_reprint"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/text_margin"
                    android:text="@string/reprint_consecutivo_f"
                    android:textSize="30sp"
                    app:fontFamily="@font/bebasneue_regular" />

                <ImageView
                    android:id="@+id/imageView10"
                    android:layout_width="0dp"
                    android:layout_height="45dp"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1"
                    android:contentDescription="@string/contentDescriptionImages"
                    app:srcCompat="@drawable/icon_edit" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/linear_editText_six_reprint"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <EditText
                    android:id="@+id/edittext_consecutivo_f_reprint"
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:hint="@string/hint_edit_text"
                    android:inputType="text"
                    app:fontFamily="@font/bebasneue_regular" />

            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="8dp"
                android:layout_marginTop="5dp"
                android:background="#CCDEC2"></View>

        </LinearLayout>

        <Button
            android:id="@+id/btn_guardar_reprint"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginTop="8dp"
            android:background="#0B8E36"
            android:fontFamily="@font/bebasneue_regular"
            android:gravity="center"
            android:text="@string/button_guardar"
            android:textColor="#FFFFFF"
            android:textSize="30sp" />

    </LinearLayout>

</android.support.constraint.ConstraintLayout>

</ScrollView>